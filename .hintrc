{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-text-size-adjust: none",
          "-webkit-overflow-scrolling",
          "-ms-filter"
        ]
      }
    ],
    "disown-opener": "off"
  }
}